mixin setterArea(id)
	div(id=id+"Setter" class="setterArea box_accordion")
		div(class="trigger")=id
		div(class="contents")
			table
				tr
					td="trigger"
					td
						select(id="animation_trigger-selectBox" class=" select")
							option(value="")
							option(value="hover")="hover"
							option(value="active")="active"
							option(value="target")="target"
							option(value="focus")="focus"
							option(value="checkd")="checkd"
				tr
					td="time"
					td
						div(id="animation_time-slider" class=" slider")
						input(id="animation_time-slider_input" class=" sliderInput" type="text" size="4" value="")

#webCustomizerArea
	div(id="cssChanger" class="webCustomizer box_accordion draggable")
		div(class="trigger fixed")
			i(id="cssChangerIcon" class="fa-2x fa fa-edit")
		div(class="contents")
			div(class="headerArea")
				div(class="headerFunction")
					span="element"
					input(id="webeleName_input" class="webeleName" type="text" size="10" value="none")
					span="anime"
					select(id="animationState_select" class=" select")
						option(value="start")="start"
						option(value="end")="end"
					span="all"
					input(id="animAll_checked" class="" type="checkbox")
				div(class="headerFunction")
					span="responsive"
					input(id="responsive_input" class="" type="text" size="7" value="exSmall")
					span="all"
					input(id="resAll_checked" class="" type="checkbox" checked="true")
					span="max"
					select(id="responsiveMax_select" class=" select")
						option(value="exSmall")="exSmall"
						option(value="small")="small"
						option(value="middle")="middle"
						option(value="large")="large"

			div(class="contentsArea css")
				+setterArea("animation")

